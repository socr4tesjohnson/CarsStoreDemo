{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [{
  "provider": "unpkg",
  "library": "@microsoft/signalr@latest",
  "destination": "wwwroot/js/signalr",
  "files": [
    "dist/browser/signalr.js",
    "dist/browser/signalr.min.js"
  ]
}
,
{
  "library": "vue@2.6.11",
  "destination": "wwwroot/js/vue/"
}
,
{
  "provider": "unpkg",
  "library": "bootstrap@4.4.1",
  "destination": "wwwroot/lib/bootstrap/"
}
,
{
  "provider": "unpkg",
  "library": "jquery-slim@3.0.0",
  "destination": "wwwroot/lib/jquery-slim/"
}
,
{
  "provider": "unpkg",
  "library": "bootstrap-vue@2.9.0",
  "destination": "wwwroot/lib/bootstrap-vue/"
}
]
}